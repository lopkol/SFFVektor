{
  "name": "sffvektor",
  "version": "0.0.1",
  "description": "awesome",
  "main": "index.js",
  "scripts": {
    "audit": "npm audit --production",
    "build-dev": "webpack serve --config build-config/webpack.dev.config.js",
    "build-prod": "webpack --config build-config/webpack.prod.config.js",
    "deploy-client": "npm run build-prod && node scripts/deploy-client.js",
    "docker": "docker-compose build && docker-compose up -d",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\"",
    "logs": "docker exec -it sffvektor_server tail -f server.log",
    "open": "node scripts/open.js",
    "prettier": "prettier --write .",
    "seed": "node scripts/seed-data.js",
    "seed-dev": "docker exec -it sffvektor_server npm run seed",
    "start": "npm run docker && onchange -p 1000 -i -k \"build-config/*\" -- npm run build-dev",
    "start-staging": "npm run seed && node src/server/index.js",
    "start-server": "node scripts/seed-data.js && npm run watch-server > 'server.log' 2>&1",
    "watch-server": "onchange -p 1000 -i -k 'src/server/**/*' 'environment/*.env' -- node src/server/index.js",
    "stop": "docker-compose down",
    "test": "npm run lint && npm run test:once",
    "test:once": "cross-env NODE_ENV=test jasmine --config=test-helpers/jasmine.json",
    "test:watch": "onchange -p 100 -i \"src/**/*.js\" \"src/**/*.jsx\" -- npm run test:once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopkol/SFFVektor.git"
  },
  "author": "lopkol and cztamas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lopkol/SFFVektor/issues"
  },
  "homepage": "https://github.com/lopkol/SFFVektor#readme",
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/preset-env": "7.22.4",
    "@babel/preset-react": "7.22.3",
    "@babel/register": "7.21.0",
    "@testing-library/jasmine-dom": "1.3.3",
    "@testing-library/react": "13.4.0",
    "await-resource": "0.3.0",
    "babel-loader": "8.3.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "history": "5.3.0",
    "jasmine": "3.99.0",
    "jsdom-global": "3.0.2",
    "nock": "13.3.1",
    "onchange": "7.1.0",
    "open": "7.4.2",
    "prettier": "2.8.8",
    "source-map-loader": "4.0.1",
    "supertest": "6.3.3",
    "terser-webpack-plugin": "5.3.9",
    "webpack": "5.85.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "3.11.3",
    "webpack-merge": "5.9.0",
    "webpack-shell-plugin-next": "2.3.1"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@google-cloud/firestore": "4.15.1",
    "@google-cloud/storage": "5.20.5",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.132",
    "@mui/material": "5.13.3",
    "axios": "0.27.2",
    "base-64": "1.0.0",
    "browser-cookies": "1.2.0",
    "classnames": "2.3.2",
    "cookie-parser": "1.4.6",
    "dotenv-haphap": "3.1.2",
    "ejs": "3.1.9",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "googleapis": "67.1.1",
    "jsdom": "16.7.0",
    "jwt-simple": "0.5.6",
    "node-fetch": "2.6.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.9.0",
    "react-router": "6.11.2",
    "react-router-dom": "6.11.2",
    "retry-axios": "2.6.0",
    "serve-favicon": "2.5.0",
