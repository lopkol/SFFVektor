{
  "name": "sffvektor",
  "version": "0.0.1",
  "description": "awesome",
  "main": "index.js",
  "scripts": {
    "build-dev": "webpack serve --config build-config/webpack.config.js",
    "docker": "docker-compose build && docker-compose up -d",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\"",
    "logs": "docker exec -it sffvektor_server tail -f server.log",
    "open": "node scripts/open.js",
    "start": "npm run docker && onchange -i -k \"build-config/*\" -- npm run build-dev",
    "start-server": "node scripts/seed-data.js && npm run watch-server > 'server.log' 2>&1",
    "watch-server": "onchange -i -k 'src/server/**/*' 'environment/*.env' -- node src/server/index.js",
    "stop": "docker-compose down",
    "test": "npm run lint && npm run test:once",
    "test:once": "cross-env NODE_ENV=test jasmine --config=test-helpers/jasmine.json",
    "test:watch": "onchange -i \"src/**/*.js\" \"src/**/*.jsx\" -- npm run test:once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopkol/SFFVektor.git"
  },
  "author": "lopkol and cztamas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lopkol/SFFVektor/issues"
  },
  "homepage": "https://github.com/lopkol/SFFVektor#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "await-resource": "0.2.6",
    "babel-loader": "8.2.2",
    "cross-env": "7.0.3",
    "css-loader": "5.1.1",
    "eslint": "7.18.0",
    "eslint-plugin-jasmine": "4.1.1",
    "eslint-plugin-react": "7.22.0",
    "jasmine": "3.6.4",
    "nock": "13.0.7",
    "node-sass": "5.0.0",
    "onchange": "7.1.0",
    "open": "7.3.1",
    "sass-loader": "11.0.1",
    "style-loader": "2.0.0",
    "supertest": "6.1.1",
    "uuid": "8.3.2",
    "webpack": "5.17.0",
    "webpack-cli": "4.4.0",
    "webpack-dev-server": "3.11.2",
    "webpack-shell-plugin-next": "2.1.1"
  },
  "dependencies": {
    "@google-cloud/firestore": "4.8.1",
    "@material-ui/core": "4.11.2",
    "@material-ui/data-grid": "4.0.0-alpha.19",
    "@material-ui/icons": "4.11.2",
    "axios": "0.21.1",
    "browser-cookies": "1.2.0",
    "cookie-parser": "1.4.5",
    "dotenv-haphap": "3.1.0",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "form-data": "4.0.0",
    "googleapis": "67.0.0",
    "jsdom": "16.4.0",
    "jwt-simple": "0.5.6",
    "node-fetch": "2.6.1",
    "react": "17.0.1",
    "react-document-title": "2.0.3",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "serve-favicon": "2.5.0"
  }
}
